{
    "type": "object",
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "node:SWITCH.SWMODEL1",
    "description": "Schema of an example switch model.",
    "additionalProperties": false,
    "required": ["type", "model"],
    "properties": {
        "type": {
            "description": "Type of the node. It should be a form data argument `node_type` and not appear in the input payload JSON.",
            "type": "string",
            "enum": ["SWITCH"]
        },
        "protocol": {
            "description": "Protocol of the node. It should be generated by the schema default values and not appear in the input payload JSON.",
            "not": {},
            "default": "ETH"
        },
        "name": {
            "type": "string",
            "description": "Name of the node.",
            "default": ""
        },
        "IP": {
            "type": "string",
            "pattern": "^$|^(([0-9]|[0-9]{2}|[0-9]{3}|1[0-9]{2}|2[0-4][0-9]|25[0-5])\\.){3}([0-9]|[0-9]{2}|[0-9]{3}|1[0-9]{2}|2[0-4][0-9]|25[0-5])$",
            "$comment": "The regex is from this answer https://stackoverflow.com/a/106223",
            "description": "IPV4 address of the node. According to dotted-quad ABNF syntax as defined in RFC 2673, section 3.2.",
            "default": ""
        },
        "MAC": {
            "type": "string",
            "pattern": "^$|^((([a-fA-F0-9]{2}-){5}([a-fA-F0-9]{2}))|(([a-fA-F0-9]{2}:){5}([a-fA-F0-9]{2})))$",
            "$comment": "The regex is from this answer https://stackoverflow.com/a/55532861. Note that the answer has a small mistake.",
            "description": "MAC address of the node. It should be six groups of two hexadecimal digits, separated by hyphens (-) or colon (:).",
            "default": ""
        },
        "model": {
            "description": "Model name of the switch. It should be a form data argument `model` and not appear in the input payload JSON.",
            "type": "string",
            "enum": ["SWMODEL1"]
        },
        "ports": {
            "type": "object",
            "description": "Physical ports of the switch, it should be generated by the schema default values and not appear in the input payload JSON.",
            "not": {},
            "additionalProperties": false,
            "patternProperties": {
                ".*": {
                    "type": "object",
                    "description": "Specification of a physical port.",
                    "additionalProperties": false,
                    "properties": {
                        "protocol": {
                            "description": "Protocol of the link that connects to the port.",
                            "type": "string",
                            "enum": ["ETH"]
                        },
                        "port_type": {
                            "description": "Circuit type of the port.",
                            "type": "string"
                        },
                        "available_bandwidth": {
                            "description": "Maximum bandwidth of the port in MB/s.",
                            "type": "number",
                            "minimum": 0
                        },
                        "port_bit": {
                            "description": "Physical port number of the switch. Internal use only.",
                            "type": "integer",
                            "minimum": 0
                        }
                    }
                }
            },
            "default": {
                "ETH_A1": {
                    "protocol": "ETH",
                    "port_type": "10GBASE-T",
                    "available_bandwidth": 10000,
                    "port_bit": 0
                },
                "ETH_A2": {
                    "protocol": "ETH",
                    "port_type": "10GBASE-T",
                    "available_bandwidth": 10000,
                    "port_bit": 1
                },
                "ETH_B1": {
                    "protocol": "ETH",
                    "port_type": "5GBASE-T",
                    "available_bandwidth": 5000,
                    "port_bit": 2
                },
                "ETH_B2": {
                    "protocol": "ETH",
                    "port_type": "5GBASE-T",
                    "available_bandwidth": 5000,
                    "port_bit": 3
                },
                "ETH_C1": {
                    "protocol": "ETH",
                    "port_type": "2.5GBASE-T",
                    "available_bandwidth": 2500,
                    "port_bit": 4
                },
                "ETH_C2": {
                    "protocol": "ETH",
                    "port_type": "2.5GBASE-T",
                    "available_bandwidth": 2500,
                    "port_bit": 5
                },
                "ETH_D1": {
                    "protocol": "ETH",
                    "port_type": "1000BASE-T1",
                    "available_bandwidth": 1000,
                    "port_bit": 6
                },
                "ETH_D2": {
                    "protocol": "ETH",
                    "port_type": "1000BASE-T1",
                    "available_bandwidth": 1000,
                    "port_bit": 7
                },
                "ETH_D3": {
                    "protocol": "ETH",
                    "port_type": "1000BASE-T1",
                    "available_bandwidth": 1000,
                    "port_bit": 8
                },
                "ETH_D4": {
                    "protocol": "ETH",
                    "port_type": "1000BASE-T1",
                    "available_bandwidth": 1000,
                    "port_bit": 9
                },
                "ETH_D5": {
                    "protocol": "ETH",
                    "port_type": "1000BASE-T1",
                    "available_bandwidth": 1000,
                    "port_bit": 10
                },
                "ETH_D6": {
                    "protocol": "ETH",
                    "port_type": "1000BASE-T1",
                    "available_bandwidth": 1000,
                    "port_bit": 11
                },
                "ETH_E1": {
                    "protocol": "ETH",
                    "port_type": "100BASE-T1",
                    "available_bandwidth": 100,
                    "port_bit": 12
                },
                "ETH_E2": {
                    "protocol": "ETH",
                    "port_type": "100BASE-T1",
                    "available_bandwidth": 100,
                    "port_bit": 13
                },
                "ETH_E3": {
                    "protocol": "ETH",
                    "port_type": "100BASE-T1",
                    "available_bandwidth": 100,
                    "port_bit": 14
                },
                "ETH_E4": {
                    "protocol": "ETH",
                    "port_type": "100BASE-T1",
                    "available_bandwidth": 100,
                    "port_bit": 15
                },
                "ETH_E5": {
                    "protocol": "ETH",
                    "port_type": "100BASE-T1",
                    "available_bandwidth": 100,
                    "port_bit": 16
                },
                "ETH_E6": {
                    "protocol": "ETH",
                    "port_type": "100BASE-T1",
                    "available_bandwidth": 100,
                    "port_bit": 17
                },
                "ETH_E7": {
                    "protocol": "ETH",
                    "port_type": "100BASE-T1",
                    "available_bandwidth": 100,
                    "port_bit": 18
                },
                "ETH_E8": {
                    "protocol": "ETH",
                    "port_type": "100BASE-T1",
                    "available_bandwidth": 100,
                    "port_bit": 19
                }
            }
        }
    }
}